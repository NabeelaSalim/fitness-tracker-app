      </Label>
      <DatePicker fx:id="datePicker" layoutX="120.0" layoutY="98.0" />
      <Button fx:id="btn_did" layoutX="584.0" layoutY="287.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="168.0" styleClass="button-blue" text="I did workout" />
      <Button fx:id="btn_skipped" layoutX="373.0" layoutY="287.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="185.0" styleClass="button-blue" stylesheets="@FJStyle.css" text="I skipped workout" />
