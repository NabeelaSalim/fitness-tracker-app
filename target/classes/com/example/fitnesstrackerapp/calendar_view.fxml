      <ImageView fitHeight="252.0" fitWidth="311.0" layoutX="500.0" layoutY="-6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/undraw_calendar_76t8-removebg-preview.png" />
